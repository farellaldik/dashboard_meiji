                        });

                        // Tombol "Kecilkan"
                        $('#show-less').off('click').on('click', function () {
                            activityContainer.empty(); // Hapus isi sebelumnya
                            var visibleItems = visits.slice(0, 5);
                            visibleItems.forEach(function (visit) {
                                var badgeClass = visit.visitStatus === 'Done' ? 'text-success' : 'text-danger';
                                var activityItem = `
                        <div class="activity-item d-flex">
                            <div class="activite-label">${visit.visitStatus}</div>
                            <i class="bi bi-circle-fill activity-badge ${badgeClass} align-self-start"></i>
                            <div class="activity-content">
                                <a href="#" class="fw-bold text-dark">${visit.doctorName}</a> <br>
                                ${visit.practiceName} <br>
                                <a class="fw-bold text-dark">${visit.prodID}</a>
                            </div>
                        </div><!-- End activity item-->
                        `;
                                activityContainer.append(activityItem);
                            });
                            $('#show-less').hide(); // Sembunyikan tombol "Kecilkan"
                            $('#show-more').show(); // Tampilkan tombol "Selengkapnya"
                        });


                    },
                    error: function (error) {
                        console.log("Error fetching data: ", error);
                    }
                });

                $.ajax({
                    url: '@Url.Action("Index", "Dashboard")',  // Ganti dengan URL dari JSON kamu
                    type: 'GET',
                    dataType: 'json',
                    data: { employeeNik: employeeNik },
                    success: function (data) {
                        var visits = data.visitLater;
                        var activityContainer = $('#activity-container2');
                        var placeholder = $('#empty-placeholder2');

                        activityContainer.empty();

                        if (visits.length === 0) {
                            // Jika VisitLater kosong, tampilkan gambar dan pesan
                            placeholder.show();
                            $('#show-more2').hide();
                            $('#show-less2').hide();
                        } else {
                            // Jika VisitLater ada isinya, sembunyikan placeholder dan tampilkan activity items
                            placeholder.hide();
                            var visibleItems = visits.slice(0, 5);
                            visibleItems.forEach(function (visit) {
                                var badgeClass = visit.visitStatus === 'Done' ? 'text-success' : 'text-info';
                                var activityItem = `
                                        <div class="activity-item d-flex">
                                            <div class="activite-label">${visit.dateVisit}</div>
                                            <i class="bi bi-circle-fill activity-badge ${badgeClass} align-self-start"></i>
                                            <div class="activity-content">
                                                <a href="#" class="fw-bold text-dark">${visit.doctorName}</a> <br>
                                                ${visit.practiceName} <br>
                                                <a class="fw-bold text-dark">${visit.prodID}</a>
                                            </div>
                                        </div><!-- End activity item-->
                                    `;
                                activityContainer.append(activityItem);
                            });

                            // Tampilkan tombol jika ada lebih dari 5 item
                            if (visits.length > 5) {
                                $('#show-more2').show();
                            }
                        }

                        // Tombol "Selengkapnya"
                        $('#show-more2').off('click').on('click', function () {
                            activityContainer.empty(); // Hapus isi sebelumnya
                            visits.forEach(function (visit) {
                                var badgeClass = visit.visitStatus === 'Done' ? 'text-success' : 'text-info';
                                var activityItem = `
                                <div class="activity-item d-flex">
                                    <div class="activite-label">${visit.dateVisit}</div>
                                    <i class="bi bi-circle-fill activity-badge ${badgeClass} align-self-start"></i>
                                    <div class="activity-content">
                                        <a href="#" class="fw-bold text-dark">${visit.doctorName}</a> <br>
                                        ${visit.practiceName} <br>
                                        <a class="fw-bold text-dark">${visit.prodID}</a>
                                    </div>
                                </div><!-- End activity item-->
                                `;
                                activityContainer.append(activityItem);
                            });
                            $('#show-more2').hide(); // Sembunyikan tombol "Selengkapnya"
                            $('#show-less2').show(); // Tampilkan tombol "Kecilkan"
                        });

                        // Tombol "Kecilkan"
                        $('#show-less2').off('click').on('click', function () {
                            activityContainer.empty(); // Hapus isi sebelumnya
                            var visibleItems = visits.slice(0, 5);
                            visibleItems.forEach(function (visit) {
                                var badgeClass = visit.visitStatus === 'Done' ? 'text-success' : 'text-info';
                                var activityItem = `
                                <div class="activity-item d-flex">
                                    <div class="activite-label">${visit.dateVisit}</div>
                                    <i class="bi bi-circle-fill activity-badge ${badgeClass} align-self-start"></i>
                                    <div class="activity-content">
                                        <a href="#" class="fw-bold text-dark">${visit.doctorName}</a> <br>
                                        ${visit.practiceName} <br>
                                        <a class="fw-bold text-dark">${visit.prodID}</a>
                                    </div>
                                </div><!-- End activity item-->
                                `;
                                activityContainer.append(activityItem);
                            });
                            $('#show-less2').hide(); // Sembunyikan tombol "Kecilkan"
                            $('#show-more2').show(); // Tampilkan tombol "Selengkapnya"
                        });


                    },
                    error: function (error) {
                        console.log("Error fetching data: ", error);
                    }
                });
            }

            // Chart options
            var options = {
                series: [
                    {
                        name: 'Quiz Lulus',
                        data: [0]
                    },
                    {
                        name: 'Quiz Tidak Lulus',
                        data: [0]
                    }
                ],
                chart: {
                    type: 'bar',
                    height: 350,
                    stacked : true
                },
                plotOptions: {
                    bar: {
                        horizontal: false,
                        columnWidth: '55%',
                        endingShape: 'rounded'
                    },
                },
                dataLabels: {
                    enabled: true
                },
                stroke: {
                    show: true,
                    width: 2,
                    colors: ['transparent']
                },
                xaxis: {
                    categories: ['2024'],
                },
                yaxis: {
                    title: {
                        text: 'Jumlah Pertanyaan'
                    }
                },
                colors: ['#91cc75', '#ee6666'],
                fill: {
                    opacity: 1
                },
                tooltip: {
                    y: {
                        formatter: function (val) {
                            return val + " Quiz";
                        }
                    }
                }
            };

            var options1 = {
                series: [{
                    name: 'Grade A',
                    data: []
                }, {
                    name: 'Grade B',
                    data: []
                }, {
                    name: 'Grade C',
                    data: []
                }],
                chart: {
                    type: 'bar',
                    height: 350,
                    stacked: 'true',
                },
                plotOptions: {
                    bar: {
                        horizontal: false,
                        columnWidth: '55%',
                        endingShape: 'rounded'
                    },
                },
                dataLabels: {
                    enabled: true
                },
                stroke: {
                    show: true,
                    width: 2,
                    colors: ['transparent']
                },
                xaxis: {
                    categories: [],
                },
                yaxis: {
                    title: {
                        text: 'Dokter'
                    }
                },
                fill: {
                    opacity: 1
                },
                tooltip: {
                    y: {
                        formatter: function (val) {
                            return val + " Dokter"
                        }
                    }
                }
            };

            // Create and render the chart
            var chart = new ApexCharts(document.querySelector("#columnChartTest"), options);
            chart.render();

            var chart1 = new ApexCharts(document.querySelector("#columnChart"), options1);
            chart1.render();

            $(document).on('dropdownChanged', function (event, selectedValue) {
                $('#cardTitle').text(selectedValue);
                updateChart(selectedValue);
            });

            // Load the saved dropdown selection from sessionStorage
            var savedSelection = sessionStorage.getItem('dropdownSelection')
            if (savedSelection) {
                $('#cardTitle').text(savedSelection);
                updateChart(savedSelection);
            }

            startDate.addEventListener('change', updateVisitLater);
            endDate.addEventListener('change', updateVisitLater);

        });
    </script>

</body>

